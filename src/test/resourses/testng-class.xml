<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Page Object test example">

    <test name="MailTest">
        <parameter name="appURL" value="http://google.com/"/>
        <classes>
            <class name="com.pack.common.tests.SearchPageTest"/>
        </classes>
    </test>

    <test name="SimpleTest">
    <parameter name="appURL" value="http://mail.google.com/"/>
